<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/level4_rhythm_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.android.dictations.Level4Rhythm">


    <TextView
        android:id="@+id/l4rTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:text="Level 4 Rhythm Dictations"
        android:textAlignment="center"
        android:textSize="20sp" />

    <ScrollView
        android:id="@+id/l4rScrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Switch
                android:id="@+id/l4r1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="1. 3/4 Quarter Note Quadruplet" />

            <Switch
                android:id="@+id/l4r2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="2. 3/4 Quarter Note Quadruplet" />

            <Switch
                android:id="@+id/l4r3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="3. 3/4 Quarter Note Triplet and Quadruplet" />

            <Switch
                android:id="@+id/l4r4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="4. 3/4 Quarter Note Quadruplet with rests" />

            <Switch
                android:id="@+id/l4r5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="5. 4/4 Quarter Note Tripet and Quadruplet" />

            <Switch
                android:id="@+id/l4r6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="6. 12/8 Three tied notes in the space of four dotted quarter notes)" />

            <Switch
                android:id="@+id/l4r7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="7. 4/4 Half Note Triplet" />

            <Switch
                android:id="@+id/l4r8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="8. 4/4 Half Note Triplet" />

            <Switch
                android:id="@+id/l4r9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="9. 4/4 Half Note Triplet" />

            <Switch
                android:id="@+id/l4r10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="10. 4/4 Half and Quarter Note Triplets" />

            <Switch
                android:id="@+id/l4r11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="11. 3/4 Advanced Syncopation" />

            <Switch
                android:id="@+id/l4r12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="12. 4/4 Advanced Syncopation" />

            <Switch
                android:id="@+id/l4r13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="13. 2/4 Advanced Syncopation" />

            <Switch
                android:id="@+id/l4r14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="14. 6/8 Advanced Syncopation" />

            <Switch
                android:id="@+id/l4r15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="15. 3/4 Hemiola" />

            <Switch
                android:id="@+id/l4r16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="16. 6/8 Hemiola" />

            <Switch
                android:id="@+id/l4r17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="17. 2/4 Sixteenth Note Quintuplets" />

            <Switch
                android:id="@+id/l4r18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="18. 3/4 Sixteenth Note Quintuplets" />

            <Switch
                android:id="@+id/l4r19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="19. 4/4 Eighth Note Quintuplets" />

            <Switch
                android:id="@+id/l4r20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="20. 3/4 Eighth Note Quintuplets" />

            <Switch
                android:id="@+id/l4r21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="21. 3/4 Sixteenth Note Septuplets" />

            <Switch
                android:id="@+id/l4r22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="22. 4/4 Sixteenth Note Septuplets" />

            <Switch
                android:id="@+id/l4r23"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="23. 5/8 Consistent groupings of 3 + 2" />

            <Switch
                android:id="@+id/l4r24"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="24. 5/8 Consistent groupings of 2 + 3" />

            <Switch
                android:id="@+id/l4r25"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="25. 7/8 Consistent groupings of 2 + 2 + 3" />

            <Switch
                android:id="@+id/l4r26"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="26. 7/8 Consistent groupings of 3 + 2 + 2" />

            <Switch
                android:id="@+id/l4r27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="27. 8/8 Consistent groupings of 3 + 2 + 3" />

            <Switch
                android:id="@+id/l4r28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="28. 9/8 Consistent groupings of 2 + 2 + 2 + 3" />

            <Switch
                android:id="@+id/l4r29"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="29. 11/8 Consistent groupings of 2 + 2 + 2 + 2 + 3" />

            <Switch
                android:id="@+id/l4r30"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="dictationChosen"
                android:padding="10dp"
                android:text="30. 11/8 Consistent groupings of 3 + 2 + 2 + 2 + 2" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>
